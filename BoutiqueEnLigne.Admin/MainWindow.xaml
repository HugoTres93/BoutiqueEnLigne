<Window x:Class="BoutiqueEnLigne.Admin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoutiqueEnLigne.Admin"
        mc:Ignorable="d"
        Title="Boutique en ligne" Height="450" Width="800">
    <DockPanel LastChildFill="False">
        <Menu x:Name="menu1" Height="30" Width="Auto" DockPanel.Dock="Top" Background="White" Foreground="Black">
            
            <MenuItem x:Name="Utilisateur" Header="Gerer un Utilisateur" Click="gereUtilisateur_Click">
            </MenuItem>
            <MenuItem x:Name="Produit" Header="Gerer les Produits" Click="gereProduit_Click">
            </MenuItem>

        </Menu>
    </DockPanel>
</Window>
